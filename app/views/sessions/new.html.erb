<h1> Log In Here! </h1>

<%= form_with(url:'/login') do |f| %>
    <%= f.label :email %> 
    <%= f.email_field :email %> <br> <br>

    <%= f.label :password %> <br>
    <%= f.password_field :password %> <br>

    <%= f.submit "Log In"%>

<% end %>

<h1> Or Sign Up <%= link_to("here!", signup_path) %></h1>
